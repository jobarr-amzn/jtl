{
	!init: {
		csv: module("csv", {
			format: "Default", // DEFAULT EXCEL MYSQL RFC4180 TDF
			charset: "UTF-8" // any names java charset
		})
	},
	tonn: toN(number()),
	toN: $1(.)/($1() ? . : $_),
	tonum: number(.)/(number() ? . : $_),
	$cleaned : csv.read($1)/*/map(tonn()),
	_: {
		data: $cleaned
	}
}
